pipeline {
    agent any

    parameters {
    string(name: 'ACCOUNT', defaultValue: '', description: 'AWS Account Name')
    string(name: 'ID', defaultValue: '', description: 'AWS Account ID')
    }

    stages {
        stage("Add Account") {
            steps {
               sh '''
               pip3 install mysql-connector-python
               python3 add_account.py
               '''
            }
        }
    }
}